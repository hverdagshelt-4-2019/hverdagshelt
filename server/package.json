{
  "name": "hverdagshelt-server",
  "version": "0.0.1",
  "scripts": {
    "start": "nodemon --exec babel-node src/index.js",
    "start2": "nodemon --exec babel-node test/dao/fetchtesting.js",
    "start-prod": "cross-env NODE_ENV=production babel-node src/index.js",
    "test": "jest --detectOpenHandles --forceExit --runInBand"
  },
  "jest": {
    "transform": {
      ".js$": "babel-7-jest"
    },
    "testRegex": "./test/.+Test\\.js$",
    "verbose": true,
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ]
  },
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-flow": "^7.0.0",
    "argon2": "0.19.3",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "express-fileupload": "^1.0.0-alpha.1",
    "json5": "^2.1.0",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.11",
    "mysql2": "^1.6.1",
    "nodemailer": "^5.1.1",
    "fs": "^0.0.1-security",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "babel-7-jest": "^21.3.3",
    "babel-core": "^7.0.0-bridge.0",
    "cross-env": "^5.2.0",
    "flow-bin": "^0.90.0",
    "jest": "^23.6.0",
    "node-fetch": "^2.3.0",
    "nodemon": "^1.18.4"
  }
}
